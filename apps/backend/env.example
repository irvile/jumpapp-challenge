# Environment Configuration for Backend
# Copy this file to .env.development.local and fill in the required values

# Application Environment
NODE_ENV=development
BACKEND_PORT=3005

# Database Configuration
DATABASE_URL=postgresql://prisma:prisma@localhost:5440/prisma

# Authentication URLs
BETTER_AUTH_URL=http://localhost:3005
BACKEND_API_URL=http://localhost:3005

# Google OAuth (Required for Google Calendar integration)
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# AI Services
GEMINI_API_KEY=your_gemini_api_key_here
RECALL_API_KEY=your_recall_api_key_here

# Social Media APIs (Optional - for enhanced features)
LINKEDIN_CLIENT_ID=your_linkedin_client_id_here
LINKEDIN_CLIENT_SECRET=your_linkedin_client_secret_here

FACEBOOK_CLIENT_ID=your_facebook_client_id_here
FACEBOOK_CLIENT_SECRET=your_facebook_client_secret_here
